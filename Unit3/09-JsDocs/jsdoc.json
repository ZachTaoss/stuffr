{
    "plugins":["plugins/markdown"],
    "recurseDepth": 10,
    "source":{
        "include":"dist",
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "sourceType":"module",
    "tags":{
        "allowUnknownTags": true,
        "dictionaries":["jsdocs","closure"]
    },
    "templates": {
        "cleverLinks":true,
        "monospaceLinks":true
    },
    "opts":{
        "destination":"./docs/",
        "recurse": true
    }
}